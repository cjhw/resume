{
  "name": "resume",
  "version": "1.0.0",
  "author": "cjhw",
  "description": "",
  "main": "./dist/electron.js",
  "scripts": {
    "start:main": "webpack --config ./webpack/webpack.main.dev.js && electron ./dist/electron.js",
    "start:render": "webpack-dev-server --config ./webpack/webpack.render.dev.js",
    "build:main": "webpack --config ./webpack/webpack.main.prod.js",
    "build:render": "webpack --config ./webpack/webpack.render.prod.js",
    "build:dll": "webpack --config ./webpack/webpack.dll.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "ResumeMook.ElectronReact",
    "productName": "ResumeMook",
    "copyright": "Copyright Â© 2022 ${author}",
    "extends": null,
    "files": [
      "dist/**/*",
      "package.json",
      "node_modules/"
    ],
    "directories": {
      "output": "package"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 410,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@reduxjs/toolkit": "^1.8.6",
    "classnames": "^2.3.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-activation": "^0.12.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.0",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.9.0",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.8",
    "@types/react": "^18.0.24",
    "@types/react-css-modules": "^4.6.4",
    "@types/react-dom": "^18.0.8",
    "add-asset-html-webpack-plugin": "^5.0.2",
    "autoprefixer": "9.0.0",
    "babel-loader": "^9.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.26.0",
    "eslint-plugin-standard": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.1",
    "path-to-regexp": "^6.2.1",
    "postcss": "^8.4.18",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.0.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "thread-loader": "^3.0.4",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
